{
    "manifest_version": 2,
    "name": "FlashBug!", 
    "version": "1.0", 
    "description": "Replaces New Tabs with a customisable 60 fps 2D isosurface moving background.",
    "permissions": ["storage"],
    "browser_action":{
        "default_popup":"popup/popup.html"
    },
    "chrome_url_overrides": { 
        "newtab":"newTabPage/metaballs.html"
    },
    "icons": { 
        "16":"Icons/MetaBlue_16.png",
        "48":"Icons/MetaBlue_48.png", 
        "128":"Icons/MetaBlue_128.png" 
    }
}